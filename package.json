{
  "name": "web-img-optimizer",
  "version": "0.4.1",
  "description": "A tool for fetching and optimize all images of a web page.",
  "keywords": [
    "web",
    "image",
    "performance",
    "cli",
    "oclif"
  ],
  "license": "Unlicense",
  "homepage": "https://github.com/IGassmann/web-img-optimizer",
  "repository": "git://github.com/IGassmann/web-img-optimizer.git",
  "bugs": "https://github.com/IGassmann/web-img-optimizer/issues",
  "author": "Igor Gassmann <igor@igassmann.me> (https://igassmann.me/)",
  "files": [
    "/bin",
    "/lib",
    "/build"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "wio": "./bin/run"
  },
  "scripts": {
    "postinstall": "node ./build/download-chrome.js",
    "test": "jest",
    "build": "oclif-dev pack",
    "build:macos": "oclif-dev pack:macos",
    "build:win": "oclif-dev pack:win",
    "build:deb": "oclif-dev pack:deb",
    "version": "oclif-dev readme && git add README.md",
    "prepack": "rm -rf lib && tsc -b"
  },
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-not-found": "^1.2.4",
    "@oclif/plugin-update": "^1.3.10",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "cli-ux": "^5.6.2",
    "file-type": "^16.5.0",
    "imagemin": "^7.0.1",
    "imagemin-giflossy": "^5.1.10",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-pngcrush": "^7.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^9.0.0",
    "imagemin-zopfli": "^7.0.0",
    "node-fetch": "^2.6.1",
    "puppeteer": "^10.0.0",
    "sharp": "^0.28.3",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "IGassmann/dev-cli#master",
    "@oclif/test": "^1",
    "@types/chai": "^4.2.18",
    "@types/imagemin": "^7.0.0",
    "@types/imagemin-mozjpeg": "^8.0.0",
    "@types/imagemin-svgo": "^9.0.0",
    "@types/imagemin-zopfli": "^7.0.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^14.1.0",
    "@types/node-fetch": "^2.5.10",
    "@types/sharp": "^0.28.3",
    "chai": "^4.3.4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "jest": "^27.0.4",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.3"
  },
  "engines": {
    "node": "^14.1.0"
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "wio",
    "plugins": [
      "@oclif/plugin-update",
      "@oclif/plugin-warn-if-update-available"
    ],
    "update": {
      "s3": {
        "bucket": "web-img-optimizer-releases"
      }
    },
    "macos": {
      "identifier": "me.igassmann.web-img-optimizer"
    }
  }
}
