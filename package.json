{
  "name": "web-img-optimizer",
  "version": "0.5.2",
  "description": "A tool for optimizing the images of a web page.",
  "keywords": [
    "web",
    "image",
    "performance",
    "cli"
  ],
  "license": "Unlicense",
  "homepage": "https://github.com/IGassmann/web-img-optimizer",
  "repository": "git://github.com/IGassmann/web-img-optimizer.git",
  "bugs": "https://github.com/IGassmann/web-img-optimizer/issues",
  "author": "Igor Gassmann <igor@igassmann.me> (https://igassmann.me/)",
  "files": [
    "/bin",
    "/lib",
    "/build"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "wio": "./bin/run"
  },
  "scripts": {
    "postinstall": "node ./build/download-chrome.js",
    "test": "jest",
    "format": "prettier . --write --ignore-path ./.gitignore",
    "lint": "eslint . --ext .js,.ts --fix --ignore-path ./.gitignore",
    "build": "oclif-dev pack",
    "build:macos": "oclif-dev pack:macos",
    "build:win": "oclif-dev pack:win",
    "build:deb": "oclif-dev pack:deb",
    "prepack": "rm -rf lib && tsc -b",
    "prepare": "husky install"
  },
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-not-found": "^1.2.4",
    "@oclif/plugin-update": "^1.3.10",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "cli-ux": "^5.6.3",
    "file-type": "^16.5.1",
    "imagemin": "^7.0.1",
    "imagemin-giflossy": "^5.1.10",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^9.0.0",
    "imagemin-zopfli": "^7.0.0",
    "node-fetch": "^2.6.1",
    "puppeteer": "^10.1.0",
    "registry-js": "^1.15.1",
    "sharp": "^0.28.3",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@oclif/dev-cli": "IGassmann/dev-cli#master",
    "@oclif/test": "^1",
    "@types/chai": "^4.2.21",
    "@types/imagemin": "^7.0.1",
    "@types/imagemin-mozjpeg": "^8.0.1",
    "@types/imagemin-svgo": "^9.0.1",
    "@types/imagemin-zopfli": "^7.0.1",
    "@types/jest": "^26.0.24",
    "@types/node": "^14.1.0",
    "@types/node-fetch": "^2.5.11",
    "@types/sharp": "^0.28.4",
    "chai": "^4.3.4",
    "eslint": "^7.30.0",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.2.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": ">=6",
    "jest": "^27.0.6",
    "lint-staged": ">=10",
    "prettier": "2.3.2",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "resolutions": {
    "**/os-filter-obj": "IGassmann/os-filter-obj#master",
    "**/pngquant-bin": "IGassmann/pngquant-bin#main",
    "**/zopflipng-bin": "IGassmann/zopflipng-bin#main",
    "**/mozjpeg-bin": "IGassmann/mozjpeg-bin#main",
    "**/mozjpeg": "IGassmann/mozjpeg-bin#main",
    "**/giflossy": "IGassmann/gifsicle-bin#main"
  },
  "engines": {
    "node": "^14.1.0"
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "wio",
    "plugins": [
      "@oclif/plugin-not-found",
      "@oclif/plugin-update",
      "@oclif/plugin-warn-if-update-available"
    ],
    "update": {
      "s3": {
        "bucket": "web-img-optimizer-releases"
      }
    },
    "macos": {
      "sign": "Developer ID Installer: Igor Gassmann (FBTMM9G54Z)",
      "identifier": "me.igassmann.web-img-optimizer"
    }
  }
}
