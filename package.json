{
  "name": "web-img-optimizer",
  "description": "A tool for fetching and optimize all images of a web page.",
  "version": "0.1.0",
  "author": "Igor Gassmann <igor@igassmann.me> (https://igassmann.me/)",
  "bin": {
    "wio": "./bin/run"
  },
  "bugs": "https://github.com/IGassmann/web-img-optimizer/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-autocomplete": "^0.3.0",
    "@oclif/plugin-help": "^3",
    "@oclif/plugin-not-found": "^1.2.4",
    "@oclif/plugin-update": "^1.3.10",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "cli-ux": "^5.6.2",
    "file-type": "^16.5.0",
    "imagemin": "^7.0.1",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-pngcrush": "^7.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^9.0.0",
    "imagemin-zopfli": "^7.0.0",
    "node-fetch": "^2.6.1",
    "puppeteer": "^10.0.0",
    "sharp": "^0.28.3",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4.2.18",
    "@types/imagemin": "^7.0.0",
    "@types/imagemin-mozjpeg": "^8.0.0",
    "@types/imagemin-svgo": "^9.0.0",
    "@types/imagemin-zopfli": "^7.0.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^14",
    "@types/node-fetch": "^2.5.10",
    "@types/sharp": "^0.28.3",
    "chai": "^4.3.4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "imagemin-giflossy": "^5.1.10",
    "jest": "^27.0.4",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/IGassmann/web-img-optimizer",
  "keywords": [
    "oclif"
  ],
  "license": "Unlicense",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "wio",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-warn-if-update-available",
      "@oclif/plugin-autocomplete"
    ],
    "update": {
      "s3": {
        "host": ""
      }
    },
    "macos": {
      "identifier": "me.igassmann.web-img-optimizer"
    }
  },
  "repository": "IGassmann/web-img-optimizer",
  "scripts": {
    "prepack": "rm -rf lib && tsc -b && oclif-dev readme",
    "test": "jest",
    "build": "oclif-dev pack",
    "build:macos": "oclif-dev pack:macos",
    "build:win": "oclif-dev pack:win",
    "build:deb": "oclif-dev pack:deb",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
